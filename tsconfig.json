{
  "compilerOptions": {
    "target": "es2017",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "module": "es2020",
    "moduleResolution": "node",
    "typeRoots": ["./types"],
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
